        "Color": null,
        "Font": null,
        "FontSize": 10
      },
      "LineParts": [
        {
          "PartText": "He\u0027s rewarded in this virtual currency, that he himself can spend on the site...",
          "Description": null
        }
      ],
      "IsThought": false,
      "Paragraphs": null,
      "Text": null,
      "Items": null,
      "IsNumbered": null
    },
