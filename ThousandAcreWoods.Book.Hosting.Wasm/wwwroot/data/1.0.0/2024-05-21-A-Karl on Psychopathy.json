    {
      "ContentType": 2,
      "Title": null,
      "Character": {
        "CharacterKey": "everyone",
        "CharacterName": "Everyone",
        "Color": null,
        "Font": null,
        "FontSize": 10
      },
      "LineParts": [
        {
          "PartText": "...",
          "Description": "a little uneasy"
