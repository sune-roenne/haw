      "IsThought": false,
      "Paragraphs": null,
      "Text": null,
      "Items": null,
      "IsNumbered": null
    },
    {
      "ContentType": 2,
      "Title": null,
      "Character": {
        "CharacterKey": "karl",
        "CharacterName": "Karl",
        "Color": null,
        "Font": null,
        "FontSize": 10
      },
      "LineParts": [
        {
          "PartText": "So, now...",
          "Description": "sitting up straight and raising his voice"
        },
        {
          "PartText": "Society as a whole would label anyone on that list a \u0027psychopath\u0027, right?",
